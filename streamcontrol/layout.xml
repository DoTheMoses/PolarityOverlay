  <label                                x=" 10" y=" 30" width="100" height="13">Minutes</label>
  <lineEdit id="timer_min"              x=" 60" y=" 30" width="480" height="20" />
  <label                                x=" 10" y=" 50" width="100" height="13">Seconds</label>
